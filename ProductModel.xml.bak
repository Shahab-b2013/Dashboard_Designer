        <Name>Viw_Org_OrganizationalChart</Name>
        <Type>DatabaseView</Type>
        <ApplySQL>
          IF OBJECT_ID (N'[Viw_Org_OrganizationalChart]') IS NOT NULL
          DROP VIEW [Viw_Org_OrganizationalChart]
          GO
          CREATE VIEW [dbo].[Viw_Org_OrganizationalChart]
          AS
          SELECT   'RegionID$' + dbo.InString(RegionID) AS ID, 'RegionID$0000000000' AS InternalID, 0 AS ParentID2, Label , Enabled, '#' AS ParentID ,'Region' AS Type, _IsDeleted,-1 _Position
          FROM        Org_Regions
          UNION ALL
          SELECT   'SubRegionID$' + dbo.InString(SubRegionID) AS ID, 'SubRegionID$0000000000' AS InternalID, 0 AS ParentID2, Label , Enabled, 'RegionID$' +dbo.InString(RegionID)  AS ParentID ,'SubRegion' AS Type, _IsDeleted,-1 _Position
          FROM        Org_SubRegions
          UNION ALL
          SELECT    'SiteID$' + dbo.InString(SiteID) AS ID, 'SiteID$0000000000' AS InternalID, 0 AS ParentID2, Label, Enabled, 'SubRegionID$' + dbo.InString(SubRegionID) AS ParentID ,'Site' AS Type, _IsDeleted,-1 _Position
          FROM          Org_Sites
          UNION ALL
          SELECT      'PadvishServerID$' + dbo.InString(PadvishServerID) AS ID, 'PadvishServerID$0000000000' AS InternalID, SiteID AS ParentID2, Label , Enabled, CASE WHEN NULL IS NOT NULL THEN 'DepartmentID$' + dbo.InString(0) ELSE 'SiteID$' + dbo.InString(SiteID) END AS ParentID
          ,'PadvishServer' AS Type, _IsDeleted,0 AS _Position
          FROM        Net_PadvishServers
        </ApplySQL>
