                  </Access>
                </Accesses>
                <Activities>
                  <Activity title="View_Detection_Chart_1">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Top Malwares</Label>
                    <ELabel>Top Malwares</ELabel>
                    <Name>View_Detection_Chart_1</Name>
                    <CommandText>
                      SELECT        TOP(10) dbo.[AbrvValue](Net_Rpt_Threats.MalwareName,'.',3) AS Abrv3_MalwareName, COUNT(*) AS COUNT
                      FROM            Net_Rpt_Threats
                      GROUP BY dbo.[AbrvValue](Net_Rpt_Threats.MalwareName,'.',3)
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
