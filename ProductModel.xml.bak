                  </Activity>
                  <Activity title="View_Computers_Chart_4">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Computers Type</Label>
                    <ELabel>Computers Type</ELabel>
                    <Name>View_Computers_Chart_4</Name>
                    <CommandText>
                      SELECT        'Workstation' AS Computer_Type, COUNT(*) AS COUNT
                      FROM            Net_Clients  WHERE  NOT (OSName LIKE '%Server%') AND  Virtualized=0
                      UNION ALL
                      SELECT        'Physical Server' AS Computer_Type, COUNT(*) AS COUNT
                      FROM            Net_Clients  WHERE   (OSName LIKE '%Server%') AND  Virtualized=0
                      UNION ALL
                      SELECT        'Virtual Server' AS Computer_Type, COUNT(*) AS COUNT
                      FROM            Net_Clients  WHERE   (OSName LIKE '%Server%') AND  Virtualized=1
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>ComputersDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Computers_Chart_5">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Computers Memory Capacity</Label>
                    <ELabel>Computers Memory Capacity</ELabel>
                    <Name>View_Computers_Chart_5</Name>
                    <CommandText>
                      SELECT        '2 GB > RAM' AS RAM_Capacity, COUNT(*) AS COUNT
                      FROM            Net_Clients  WHERE 1800 > [TotalphysicalMemory]
                      UNION ALL
                      SELECT        '4 GB > RAM > 2 GB', COUNT(*)
                      FROM           Net_Clients  WHERE [TotalphysicalMemory] BETWEEN 1800 AND 3700
                      UNION ALL
                      SELECT        '6 GB > RAM > 4 GB', COUNT(*)
                      FROM            Net_Clients  WHERE [TotalphysicalMemory] BETWEEN 3700 AND 5700
                      UNION ALL
                      SELECT        '8 GB > RAM > 6 GB', COUNT(*)
                      FROM          Net_Clients  WHERE [TotalphysicalMemory] BETWEEN 5700 AND 7700
                      UNION ALL
                      SELECT        'RAM > 8 GB', COUNT(*)
                      FROM           Net_Clients  WHERE [TotalphysicalMemory] > 7700
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>ComputersDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Computers_Chart_6">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Computers Installed CPU</Label>
                    <ELabel>Computers Installed CPU</ELabel>
                    <Name>View_Computers_Chart_6</Name>
                    <CommandText>
                      SELECT        'Intel i3' AS CPU_Type, COUNT(*) AS COUNT
                      FROM            Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%i3%')
                      UNION ALL
                      SELECT       'Intel i5', COUNT(*)
                      FROM            Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%i5%')
                      UNION ALL
                      SELECT        'Intel i7', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%i7%')
                      UNION ALL
                      SELECT        'AMD Athlon', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%AMD%')
                      UNION ALL
                      SELECT        'Intel Dual Core', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%Dual%Core%')
                      UNION ALL
                      SELECT        'Intel Core 2 Duo', COUNT(*)
                      FROM           Net_Clients
                      WHERE       (ProcessorInfo LIKE N'%Core%2%Duo%')
                      UNION ALL
                      SELECT        'Intel Core 2 Quad', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%Core%2%Quad%')
                      UNION ALL
                      SELECT       'Intel Celeron', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%Celeron%')
                      UNION ALL
                      SELECT        'Intel Pentium', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%Pentium%')
                      UNION ALL
                      SELECT        'Intel Xeon', COUNT(*)
                      FROM           Net_Clients
                      WHERE        (ProcessorInfo LIKE N'%Xeon%')
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>ComputersDashboard</SubSubject>
                    </SmartTags>
