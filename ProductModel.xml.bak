      <ActContext title="Select_Region">
        <Type>Extended</Type>
        <ContextType>SelectList</ContextType>
        <Name>Select_Region</Name>
        <DevCommand>
          SELECT Org_Regions.RegionID,Org_Regions.Label FROM Org_Regions WHERE (Org_Regions._IsDeleted = 0) AND (Org_Regions.Enabled = 1)
          AND ((Org_Regions.RegionID = @DefaultValue) OR (Org_Regions.Label LIKE N''%@SearchValue%'')) ORDER BY 2 ASC
        </DevCommand>
        <SelectList>
          <DisplayMode>Simple</DisplayMode>
        </SelectList>
        <Version>1.1.0.gui</Version>
      </ActContext>
      <ActContext title="Select_SubRegion">
        <Type>Extended</Type>
        <ContextType>SelectList</ContextType>
        <Name>Select_SubRegion</Name>
        <DevCommand>
          SELECT Org_SubRegions.SubRegionID,Org_SubRegions.Label FROM Org_SubRegions WHERE (Org_SubRegions._IsDeleted = 0) AND (Org_SubRegions.Enabled = 1)
          AND (Org_SubRegions.RegionID = @FilterValue1) ORDER BY 2 ASC
        </DevCommand>
        <SelectList>
          <DisplayMode>DropDown</DisplayMode>
        </SelectList>
        <Version>1.1.0.gui</Version>
      </ActContext>
