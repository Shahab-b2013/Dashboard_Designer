                <Activities>
                  <Activity title="View_Associated_Users_MenuItems">
                    <Type>New</Type>
                    <ContextType>CheckListView</ContextType>
                    <Label>منوی شخصی کاربر</Label>
                    <Name>View_Associated_Users_MenuItems</Name>
                    <CommandText>SELECT Sys_Prs_ShortcutMenu.ShortcutID,Sys_Gui_MenuItems.MenuItemID,Sys_Gui_MenuItems.Label,Sys_Gui_ActivityContexts.Label,Sys_Gui_MenuItems.Description FROM Sys_Gui_MenuItems INNER JOIN Sys_Prs_ShortcutMenu ON Sys_Gui_MenuItems.MenuItemID = Sys_Prs_ShortcutMenu.MenuItemID INNER JOIN Sys_Gui_ActivityContexts ON CAST(SUBSTRING([Action],43,7) AS INT)=Sys_Gui_ActivityContexts.ActivityContextID WHERE (Sys_Gui_MenuItems.Icon='fa-list' OR Sys_Gui_MenuItems.Icon='fa-plus') AND (Sys_Prs_ShortcutMenu.UserID = @ObjectID)</CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <CommandParams>
                      <ActivityParam>
                        <Label>عنوان صفحه</Label>
                        <Name>Label</Name>
                        <AttributeName>ActivityContext.Label</AttributeName>
                      </ActivityParam>
                      <ActivityParam>
                        <Name>Description</Name>
                        <AttributeName>MenuItem.Description</AttributeName>
                      </ActivityParam>
                    </CommandParams>
                    <MasterEntity>MenuItem</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>SystemManagement</Subject>
                      <SubSubject>PersonalSettingManagement</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Non_Associated_MenuItems_to_User">
                    <Type>New</Type>
                    <ContextType>DetailListView</ContextType>
                    <Label>مشاهده منو</Label>
                    <Name>View_Non_Associated_MenuItems_to_User</Name>
                    <CommandText>SELECT Sys_Gui_MenuItems.MenuItemID,Sys_Gui_ActivityContexts.Label,Sys_Gui_MenuItems.Description FROM Sys_Gui_MenuItems INNER JOIN Sys_Gui_ActivityContexts ON CAST(SUBSTRING([Action],43,7) AS INT)=Sys_Gui_ActivityContexts.ActivityContextID WHERE ((SELECT COUNT(*) FROM Sys_Prs_ShortcutMenu WHERE (Sys_Gui_MenuItems.MenuItemID = Sys_Prs_ShortcutMenu.MenuItemID) AND (Sys_Prs_ShortcutMenu.UserID = @ObjectID)) = 0) AND (Sys_Gui_MenuItems.NavigationID=1004) AND (Sys_Gui_MenuItems.Icon='fa-list' OR Sys_Gui_MenuItems.Icon='fa-plus') AND dbo.HasAccess( MenuItemID, @ObjectID ) = 1</CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <CommandParams>
                      <ActivityParam>
                        <Label>عنوان صفحه</Label>
                        <Name>Label</Name>
                        <AttributeName>ActivityContext.Label</AttributeName>
                      </ActivityParam>
                      <ActivityParam>
                        <Name>Description</Name>
                        <AttributeName>MenuItem.Description</AttributeName>
                      </ActivityParam>
                    </CommandParams>
                    <MasterEntity>MenuItem</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>SystemManagement</Subject>
                      <SubSubject>PersonalSettingManagement</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="Add_New_ShortcutMenu">
                    <Type>New</Type>
                    <ContextType>CheckFormView</ContextType>
                    <Label>افزودن صفحه به منوی شخصی</Label>
                    <Name>Add_New_ShortcutMenu</Name>
                    <CommandText>INSERT INTO Sys_Prs_ShortcutMenu(UserID,MenuItemID) SELECT  N'@ParentObjectID', MenuItemID FROM Sys_Gui_MenuItems WHERE (MenuItemID = @ObjectID)</CommandText>
                    <CommandType>InsertQuery</CommandType>
                    <MasterEntity>ShortcutMenu</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>SystemManagement</Subject>
                      <SubSubject>PersonalSettingManagement</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="Remove_ShortcutMenu">
                    <Type>New</Type>
                    <ContextType>SilentActivity</ContextType>
                    <Label>حذف صفحه از منوی شخصی</Label>
                    <Name>Remove_ShortcutMenu</Name>
                    <CommandText>DELETE FROM Sys_Prs_ShortcutMenu WHERE (ShortcutID = @ObjectID)</CommandText>
                    <CommandType>DeleteQuery</CommandType>
                    <MasterEntity>ShortcutMenu</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>SystemManagement</Subject>
                      <SubSubject>PersonalSettingManagement</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Associated_Users_ReportMenuItems">
                    <Type>New</Type>
                    <ContextType>CheckListView</ContextType>
