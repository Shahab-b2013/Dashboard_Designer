                      FROM            Net_Rpt_Threats
                      WHERE        (7 >= DATEDIFF(DD, Net_Rpt_Threats.ReceivedTime, GETDATE())) AND (dbo.[AbrvValue](Net_Rpt_Threats.MalwareName, N'.', 2) IN
