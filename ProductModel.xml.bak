          <ApplicationModel>
            <Modules>
              <AppModule>
                <Id>15002</Id>
                <Type>New</Type>
                <Label>داشبورد در یک نگاه</Label>
                <Name>OverviewDashboard</Name>
                <Version>1.1.0.srv</Version>
                <Accesses>
                  <Access>
                    <Id>150021</Id>
                    <Label>مشاهده داشبورد در یک نگاه</Label>
                    <Name>View OverviewDashboard</Name>
                    <Type>GenericRead</Type>
                  </Access>
                </Accesses>
                <Activities>
                  <Activity title="View_Overview_Chart_1">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Client Management Status</Label>
                    <ELabel>Client Management Status</ELabel>
                    <Name>View_Overview_Chart_1</Name>
                    <CommandText>
                      SELECT  Net_ClientDiscovery.Status AS Net_ClientDiscovery_Status, COUNT(*) AS COUNT
                      FROM         Net_ClientDiscovery
                      GROUP BY Net_ClientDiscovery.Status
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_2">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Client Protection Status</Label>
                    <ELabel>Client Protection Status</ELabel>
                    <Name>View_Overview_Chart_2</Name>
                    <CommandText>
                      SELECT  Net_Rpt_Clients.Status AS Net_Clients_Status, COUNT(*) AS COUNT
                      FROM         Net_Rpt_Clients
                      GROUP BY Net_Rpt_Clients.Status
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_3">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Detected Malwares in Last 7 Days</Label>
                    <ELabel>Detected Malwares in Last 7 Days</ELabel>
                    <Name>View_Overview_Chart_3</Name>
                    <CommandText>
                      SELECT        dbo.Date(Net_Rpt_Threats.ReceivedTime) AS Date_ReceivedTime, COUNT(*) AS COUNT
                      FROM            Net_Rpt_Threats
                      WHERE        (7 >= DATEDIFF(DD, Net_Rpt_Threats.ReceivedTime, GETDATE()))
                      GROUP BY dbo.Date(Net_Rpt_Threats.ReceivedTime)
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_4">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Padvish Version Status</Label>
                    <ELabel>Padvish Version Status</ELabel>
                    <Name>View_Overview_Chart_4</Name>
                    <CommandText>
                      SELECT        dbo.[bi.AbrvType](Type)  + ' ' +  dbo.[AbrvValue](ProductVersion,'.',2) AS Abrv_Type_ProductVersion, COUNT(*) AS COUNT
                      FROM            Net_Clients
                      GROUP BY dbo.[bi.AbrvType](Type)  + ' ' +  dbo.[AbrvValue](ProductVersion,'.',2)
                      ORDER BY 2 DESC
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_5">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Clients Security Status</Label>
                    <ELabel>Clients Security Status</ELabel>
                    <Name>View_Overview_Chart_5</Name>
                    <CommandText>
                      SELECT        'Database is very old' AS SecurityStatus, COUNT(*) AS COUNT
                      FROM            Net_Rpt_Clients
                      WHERE        (ComputerStatusReason LIKE N'%Database is very old%')
                      UNION ALL
                      SELECT        'DataCop backups are very old', COUNT(*)
                      FROM            Net_Rpt_Clients
                      WHERE       (ComputerStatusReason LIKE N'%DataCop backups are very old%')
                      UNION ALL
                      SELECT       'Not seen for a long time', COUNT(*)
                      FROM            Net_Rpt_Clients
                      WHERE        (ComputerStatusReason LIKE N'%Not seen for a long time%')
                      UNION ALL
                      SELECT       'Restart required', COUNT(*)
                      FROM            Net_Rpt_Clients
                      WHERE       (ComputerStatusReason LIKE N'%Restart required%')
                      UNION ALL
                      SELECT        'Some protections are off', COUNT(*)
                      FROM           Net_Rpt_Clients
                      WHERE        (ComputerStatusReason LIKE N'%Some protections are off%')
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_6">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Padvish License Usage</Label>
                    <ELabel>Padvish License Usage</ELabel>
                    <Name>View_Overview_Chart_6</Name>
                    <CommandText>
                      SELECT        dbo.[bi.AbrvType](Net_Rpt_Licenses.Type) AS Abrv_Type, SUM(AllocatedLicense)-SUM(UsedLicense) AS SUM_1, SUM(UsedLicense) AS SUM_2
                      FROM            Net_Rpt_Licenses
                      GROUP BY  dbo.[bi.AbrvType](Net_Rpt_Licenses.Type)
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_7">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>New Systems in Last 30 Days</Label>
                    <ELabel>New Systems in Last 30 Days</ELabel>
                    <Name>View_Overview_Chart_7</Name>
                    <CommandText>
                      SELECT        dbo.Date(Net_Clients.ClientRegisteredDate) AS Date_ClientRegisteredDate, COUNT(*) AS COUNT
                      FROM            Net_Clients
                      WHERE        (30 > DATEDIFF(DD, Net_Clients.ClientRegisteredDate, GETDATE()))
                      GROUP BY dbo.Date(Net_Clients.ClientRegisteredDate)
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Overview_Chart_8">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Last Seen Systems in Last 30 Days</Label>
                    <ELabel>Last Seen Systems in Last 30 Days</ELabel>
                    <Name>View_Overview_Chart_8</Name>
                    <CommandText>
                      SELECT        dbo.Date(Net_Clients.LastSeen) AS Date_LastSeen, COUNT(*) AS COUNT
                      FROM            Net_Clients
                      WHERE        (30 > DATEDIFF(DD, Net_Clients.LastSeen, GETDATE()))
                      GROUP BY dbo.Date(Net_Clients.LastSeen)
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>OverviewDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                </Activities>
              </AppModule>
              <AppModule>
                <Id>15003</Id>
                <Type>New</Type>
                <Label>داشبورد Detection</Label>
                <Name>DetectionDashboard</Name>
                <Version>1.1.0.srv</Version>
                <Accesses>
                  <Access>
                    <Id>150031</Id>
                    <Label>مشاهده داشبورد Detection</Label>
                    <Name>View DetectionDashboard</Name>
