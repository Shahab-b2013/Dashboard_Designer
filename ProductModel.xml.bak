                  <Activity title="View_Detection_Chart_2">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Anti-Crypto Detection in Last 7 Days</Label>
                    <ELabel>Anti-Crypto Detection in Last 7 Days</ELabel>
                    <Name>View_Detection_Chart_2</Name>
                    <CommandText>
                      SELECT        dbo.Date(Net_Rpt_AntiCrypto.ReceivedTime) AS Date_ReceivedTime, COUNT(*) AS COUNT
                      FROM            Net_Rpt_AntiCrypto
                      WHERE        (7 >= DATEDIFF(DD, Net_Rpt_AntiCrypto.ReceivedTime, GETDATE()))
                      GROUP BY dbo.Date(Net_Rpt_AntiCrypto.ReceivedTime)
                      ORDER BY 1
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
                    <Version>1.1.0.srv</Version>
                    <SmartTags>
                      <Subject>Dashboard</Subject>
                      <SubSubject>DetectionDashboard</SubSubject>
                    </SmartTags>
                  </Activity>
                  <Activity title="View_Detection_Chart_3">
                    <Type>New</Type>
                    <ContextType>ChartView</ContextType>
                    <Label>Clients Last Backup Status</Label>
                    <ELabel>Clients Last Backup Status</ELabel>
                    <Name>View_Detection_Chart_3</Name>
                    <CommandText>
                      SELECT       'Today' AS BackupStatus, 1 AS Position, COUNT(*) AS COUNT
                      FROM            Net_Clients
                      WHERE        (DATEDIFF(DD, LastBackupTime, GETDATE()) = 0)
                      UNION ALL
                      SELECT      '1 to 3 Days ago' , 2, COUNT(*)
                      FROM            Net_Clients
                      WHERE        (DATEDIFF(DD, LastBackupTime, GETDATE()) BETWEEN 1 AND 3)
                      UNION ALL
                      SELECT      '4 to 7 Days ago', 3, COUNT(*)
                      FROM            Net_Clients
                      WHERE        (DATEDIFF(DD, LastBackupTime, GETDATE()) BETWEEN 4 AND 7)
                      UNION ALL
                      SELECT       'More than 1 Week', 4, COUNT(*)
                      FROM         Net_Clients
                      WHERE        (DATEDIFF(DD, LastBackupTime, GETDATE()) > 7)
                      ORDER BY 2
                    </CommandText>
                    <CommandType>SelectQuery</CommandType>
                    <MasterEntity>PadvishServer</MasterEntity>
